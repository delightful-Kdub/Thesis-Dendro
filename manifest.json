{
  "manifest_version": 2,

  "name": "Dendro",
  "description": "The online research tool made just for you",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },

  "web_accessible_resources": [
  "index.html"
],

  "background":{
  "scripts": ["js/background.js"]
  },

  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "<all_urls>",
    "history"
   ]
}
